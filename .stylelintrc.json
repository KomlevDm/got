{
  "extends": "stylelint-config-recommended-scss",
  "plugins": ["stylelint-order"],
  "rules": {
    "order/order": [
      {
        "type": "at-rule",
        "name": "include",
        "parameter": "button-reset"
      },
      {
        "type": "at-rule",
        "name": "include",
        "parameter": "list-reset"
      },
      "dollar-variables",
      "custom-properties",
      "declarations",
      {
        "type": "at-rule",
        "name": "include",
        "parameter": "rtl"
      },
      {
        "type": "at-rule",
        "name": "include",
        "parameter": "rtl-prop"
      },
      {
        "type": "at-rule",
        "name": "include",
        "parameter": "theme"
      },
      {
        "type": "rule",
        "selector": "^&:(before|after)"
      },
      {
        "type": "rule",
        "selector": "^&:\\w"
      },
      {
        "type": "rule",
        "selector": "^&-"
      },
      {
        "type": "rule",
        "selector": "^."
      }
    ],
    "order/properties-alphabetical-order": null,
    "order/properties-order": [
      [
        "content",
        "position",
        "top",
        "left",
        "right",
        "bottom",
        "display",
